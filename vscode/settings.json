      "foreground": "#000000",
      "gutterIcon": true
    },
    "SKIPPED": {
      "icon": "hourglass",
      "background": "yellow",
      "iconColour": "yellow",
      "foreground": "#000000",
      "gutterIcon": true
    },
    "READ": {
      "icon": "checklist",
      "background": "#7cb9e8",
      "iconColour": "#7cb9e8",
      "foreground": "#000000",
      "gutterIcon": true
    },
    "NOTE": {
      "icon": "checklist",
      "background": "#fff338",
      "iconColour": "#fff338",
      "foreground": "#000000",
      "gutterIcon": true
    },
    "STEP": {
      "type": "line",
      "icon": "tasklist",
      "background": "#00dea4",
      "iconColour": "#00dea4",
      "foreground": "#000000",
      "gutterIcon": true
    },
    "TASK": {
      "type": "line",
      "icon": "tasklist",
      "background": "#00dea4",
      "iconColour": "#00dea4",
      "foreground": "#000000",
      "gutterIcon": true
    },
    "#": {
      "type": "line",
      "foreground": "#000000",
      "background": "#e0ff70"
    },
    "##": {
      "type": "line",
      "foreground": "#000000",
      "background": "#ff97e3"
    },
    "###": {
      "type": "line",
      "foreground": "#000000",
      "background": "#7bff89"
    }
  },
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
